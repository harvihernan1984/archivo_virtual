				return query select  res, cod;	
			END LOOP;
		else
			res='NO';
			cod='';
			return query select  res, cod;
		end if;
	end if;
	if res='SI' then res='';cod='';return query select  res, cod; end if;
	return; -- query select  res, cod;
	--select * from sis_accesos_rol_usuario('2','gd_empresa_inf','')
       
 END;
